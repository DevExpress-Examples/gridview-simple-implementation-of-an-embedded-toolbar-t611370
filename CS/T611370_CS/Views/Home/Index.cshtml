@{
    ViewBag.Title = "Home Page";
}
<script>
    function OnToolbarItemClick(s, e) {
        switch (e.item.name) {
            case "nextPage":
                s.NextPage();
                break;
            case "previousPage":
                s.PrevPage();
                break;
        }
    }
</script>

@using (Html.BeginForm("ExportTo", "Home")) {
    @Html.Action("GridViewPartial")
}
